SELECT stations.name FROM time JOIN stations ON stations.id = time.station_id WHERE train_id = 1 AND UNIX_TIMESTAMP(CONCAT(arrival_year, "-", arrival_month, "-", arrival_day," ",  MAKETIME(arrival_hour, arrival_minute, 0))) < UNIX_TIMESTAMP(CONCAT(2020, "-", 08, "-", 11, " ", MAKETIME(5, 0, 0))) LIMIT 1;